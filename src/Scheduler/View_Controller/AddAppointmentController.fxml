<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="addApptAnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Add New Appointment" underline="true">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <GridPane layoutX="14.0" layoutY="49.0" prefHeight="327.0" prefWidth="592.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="75.0" minHeight="10.0" prefHeight="75.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Appointment ID" />
            <Label text="Title" GridPane.rowIndex="1" />
            <Label text="Description" GridPane.rowIndex="2" />
            <Label text="Location" GridPane.rowIndex="3" />
            <Label text="Contact" GridPane.rowIndex="4" />
            <Label text="Type" GridPane.rowIndex="5" />
            <Label text="Start Date and Time" GridPane.columnIndex="2">
               <padding>
                  <Insets left="30.0" />
               </padding></Label>
            <TextField fx:id="appointmentIDTextField" disable="true" editable="false" GridPane.columnIndex="1" />
            <TextField fx:id="titleTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextArea fx:id="descriptionTextArea" prefHeight="65.0" prefWidth="148.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="locationTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="typeTextField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <MenuButton fx:id="contactMenuButton" mnemonicParsing="false" text="Select..." GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="End Date and Time" GridPane.columnIndex="2" GridPane.rowIndex="2">
               <padding>
                  <Insets left="30.0" />
               </padding></Label>
            <Label text="Customer" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <padding>
                  <Insets left="30.0" />
               </padding></Label>
            <Label text="User" GridPane.columnIndex="2" GridPane.rowIndex="4">
               <padding>
                  <Insets left="30.0" />
               </padding></Label>
            <TextField fx:id="customerTextField" GridPane.columnIndex="3" GridPane.rowIndex="3" />
            <TextField fx:id="userTextField" GridPane.columnIndex="3" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
