<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="14.0" layoutY="15.0" prefHeight="370.0" prefWidth="137.0">
         <children>
            <Button fx:id="addCustomerButton" mnemonicParsing="false" onAction="#switchToAddCustomerController" prefHeight="35.0" prefWidth="100.0" text="Add Customer">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" text="View Customers" />
            <Button fx:id="addAppointmentButton" mnemonicParsing="false" prefHeight="35.0" prefWidth="100.0" text="Add Appointment">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
