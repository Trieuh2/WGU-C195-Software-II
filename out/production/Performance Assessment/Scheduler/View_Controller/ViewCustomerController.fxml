<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="viewCustomerAnchorPane" prefHeight="520.0" prefWidth="906.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="customerTableView" layoutX="14.0" layoutY="14.0" prefHeight="450.0" prefWidth="875.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="75.0" text="Customer ID" />
          <TableColumn fx:id="nameColumn" prefWidth="118.0" text="Customer Name" />
            <TableColumn fx:id="addressColumn" prefWidth="141.0" text="Address" />
            <TableColumn fx:id="postalCodeColumn" minWidth="9.0" prefWidth="108.0" text="Postal Code" />
            <TableColumn fx:id="phoneNumberColumn" prefWidth="153.0" text="Phone Number" />
            <TableColumn fx:id="divisionColumn" prefWidth="174.0" text="State/Region" />
        </columns>
      </TableView>
      <Button fx:id="exitButton" layoutX="851.0" layoutY="477.0" mnemonicParsing="false" onAction="#switchToMainController" text="Exit" />
      <Button fx:id="deleteButton" layoutX="792.0" layoutY="477.0" mnemonicParsing="false" onAction="#deleteCustomer" text="Delete" AnchorPane.rightAnchor="63.0" />
      <Button fx:id="editButton" layoutX="746.0" layoutY="477.0" mnemonicParsing="false" onAction="#switchToUpdateCustomerController" text="Edit" />
   </children>
</AnchorPane>
